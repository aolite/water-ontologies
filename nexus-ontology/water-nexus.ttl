#################################################################
#    Classes
#################################################################

### https://seriousgame.sim4nexus.eu/ontology#CaseStudy
s4n:CaseStudy rdf:type owl:Class ;
              rdfs:subClassOf geosp:Feature,
                       [
                           rdf:type owl:Restriction;
                           owl:owl:onProperty s4n:regions;
                           owl:owl:allValuesFrom s4n:Region
                       ];
              rdfs:rdfs:comment "A qualitative descriptive research study of individuals or a group"@en ;
              rdfs:rdfs:isDefinedBy "https://seriousgame.sim4nexus.eu/ontology#";
              rdfs:rdfs:label "Case Study"@en .

### https://seriousgame.sim4nexus.eu/ontology#Region
s4n:Region rdf:type owl:Class ;
           rdfs:subClassOf geosp:Feature;
           rdfs:comment "The term region refers to the categorization of different geo-spatial parts "@en ;
           rdfs:rdfs:isDefinedBy "https://seriousgame.sim4nexus.eu/ontology#";
           rdfs:label "Region".




